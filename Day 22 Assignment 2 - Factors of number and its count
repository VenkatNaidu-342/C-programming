#include <stdio.h>

int main() {
    int num, i, count = 0;

    printf("Enter a positive integer: ");
    scanf("%d", &num);

    for (i = 1; i <= num; i++) {
        if (num % i == 0) {
            count++;
            printf("%d ",i);
        }
    }

    printf(" (%d)\n", count);

    return 0;
}
